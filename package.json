{
  "name": "lego-backend",
  "version": "1.0.6",
  "description": "test",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-lego-backend",
    "stop": "egg-scripts stop --title=egg-server-lego-backend",
    "dev": "egg-bin dev --port 7001",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "build:template:dev": "npx webpack --config webpack/webpack.config.js",
    "build:template:prod": "npx webpack --env production --config webpack/webpack.config.js && npm run upload",
    "upload": "node webpack/uploadToOSS.js",
    "release": "release-it"
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@types/ali-oss": "^6.16.11",
    "@types/busboy": "^1.5.4",
    "ali-oss": "^6.20.0",
    "axios": "^0.21.4",
    "busboy": "^1.6.0",
    "co-busboy": "^2.0.1",
    "dotenv": "^16.4.5",
    "egg": "^2.6.1",
    "egg-bcrypt": "^1.1.0",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-oss": "^3.1.0",
    "egg-redis": "^2.6.0",
    "egg-scripts": "^2.6.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.3.0",
    "egg-viking": "^1.0.4",
    "filemanager-webpack-plugin": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lego-components": "^0.1.7",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.10",
    "mongoose-sequence": "^6.0.1",
    "nanoid": "^3.1.30",
    "node-jsonwebtoken": "^0.0.1",
    "release-it": "^17.3.0",
    "sharp": "^0.33.4",
    "stream-wormhole": "^2.0.1",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.5",
    "@types/mocha": "^2.2.40",
    "@types/node": "^20.14.2",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "tslib": "^1.9.0",
    "typescript": "^4.4.3",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "viking zhang",
  "license": "MIT"
}
